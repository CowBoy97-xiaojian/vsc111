#定义组件名称
logstream_jituan.sources = shotpot
logstream_jituan.channels = memoryCore memoryDMZ
logstream_jituan.sinks = kafkaCore kafkaDMZ

#日志源采集设置
logstream_jituan.sources.shotpot.type = TAILDIR
logstream_jituan.sourcesshotpot.positionFile = /var/lib/flume-ng/taildir-position_for_flink_jituan.json
logstream_jituan.sources.shotpot.filegroups = 2p4f 2e4p 9o7g 5q5k
