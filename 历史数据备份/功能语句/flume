# 流通道: Kafka
logstream_jituan.sources.shotpot.channels = memoryCore memoryDMZ
logstream_jituan.sinks.kafkaCore.channel = memoryCore
logstream_jituan.sinks.kafkaDMZ.channel = memoryDMZ

# 核心域Kafka设置
logstream_jituan.sinks.kafkaCore.type = org.apache.flume.sink.kafka.KafkaSink
logstream_jituan.sinks.kafkaCore.kafka.bootstra.servers = lave-core-2:9092 slave-core-4:9092 slave-core-6:9092 siave-core-8.909
logstream_jituan.sinks.kafkaCore.kafka.topic = streams-raw
logstream_jituan.sinks.kafkaCore.flumeBatchSize = 1000
logstream_jituan.sinks.kafkaCore.kafka.producer.acks = 1
logstream_jituan.sinks.kafkaCore.kafka.producer.compression.type = snappy


# DMZ域Kafka设置
logstream_jituan.sinks.kafkaDMZ.type = org.apache.flume.sink.kafka.KafkaSink
logstream_jituan.sinks.kafkaDMZ.brokerList = zk-kfk01:9092zk-kfk02:9092,zk-kfk03:9092
logstream_jituan.sinks.kafkaDMZ.topic = streams-raw
logstream_jituan.sinks.kafkaDMzflumeBatchSize = 1000
logstream_jituan.sinks.kafkaDMZ.kafka.producer.acks = 1
logstream_jituan.sinks.kafkaDMZ.kafka.producer.compression.type = snappy


# Memorv通道设置
logstream_jituan.channels.memoryCore.type = memory
logstream_jituan.channels.memoryCore.capacity = 100000
logstream_jituan.channels.memoryCore.transactionCapacity = 20000
ogstream_jituan.channels.memoryDMZ.type = memory
logstream_jituan.channels.memoryDMZ.capacity = 100000
logstream_jituan.channels.memoryDMZ.transactionCapacity = 20000